@model Vaccine

<h1>New Doses</h1>

<form method="post">

    <div class="form-group">
        <label asp-for="Id"> Vaccine </label>
        <select asp-for="Id" asp-items="ViewBag.Vaccines" ></select>
    </div>

    <div class="form-group">
        <label asp-for="DosesRecieved"> New Doses Recieved </label>
        <input asp-for="DosesRecieved" class="form-control" value="0">
    </div>

    <button type="submit" class="btn btn-primary"> Save </button>

</form>

